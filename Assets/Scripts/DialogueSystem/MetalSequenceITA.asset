  m_Script: {fileID: 11500000, guid: 94d7290b59f84484b96604a47814552c, type: 3}
  m_Name: MetalSequenceITA
  m_EditorClassIdentifier: 
  lines:
  - characterName: Mozart
    sentence: "AH, LA MUSICA! UN LINGUAGGIO UNIVERSALE CHE CAMBIA ED EVOLVE NEL TEMPO.
      MA ORA, GIOVANE MUSICISTA, TI TROVI DI FRONTE A UNA DELLE FORME PI\xD9 POTENTI
      E RIBELLI DELLA MUSICA MODERNA: IL METAL!"
  - characterName: Cadence
    sentence: "METAL, EH? HO SENTITO PARLARE DELLA SUA POTENZA BRUTA, MA NON L\u2019HO
      MAI AFFRONTATO IN QUESTO MODO. SONO PRONTO A IMPARARE\u2026 E A SUONARE!"
  - characterName: Mozart
    sentence: "PRIMA DI AFFRONTARE QUESTA SFIDA, PERMETTIMI DI INTRODURTI AGLI STRUMENTI
      DEL METAL. NEL METAL, LA CHITARRA ELETTRICA REGNA SOVRANA! CON IL SUO SUONO
      DISTORTO E AGGRESSIVO, GUIDA LA MELODIA E CREA RIFF POTENTI.\r"
  - characterName: Cadence
    sentence: "LA CHITARRA ELETTRICA, EH? POSSO GI\xC0 IMMAGINARE L\u2019ENERGIA
      CHE DEVE AVERE. MA NON PREOCCUPARTI, NON MI LASCER\xD2 INTIMIDIRE. IMPARER\xD2
      A MUOVERMI CON LA SUA POTENZA E A SFRUTTARE I MIEI PUNTI DI FORZA PER ESSERNE
      ALL' ALTEZZA!"
  - characterName: Mozart
    sentence: POI ABBIAMO IL BASSO, CHE RAFFORZA LA BASE RITMICA CON TONI PROFONDI
      E INCALZANTI. E NON DIMENTICHIAMO LA BATTERIA! IL CUORE DEL METAL, CON RITMI
      VELOCI E POTENTI COLPI DI DOPPIA CASSA!
  - characterName: Cadence
    sentence: "IL BASSO E LA BATTERIA CREANO LA STRUTTURA PORTANTE DEL METAL, SPINGENDO
      OGNI ELEMENTO IN AVANTI CON LA LORO POTENZA. POSSO GI\xC0 SENTIRE COME GUIDINO
      IL FLUSSO DELLA MUSICA. SAPR\xD2 ALLINEARE IL MIO RITMO AL LORO, SOSTENENDO
      L\u2019ENERGIA DI OGNI BRANO!"
  - characterName: Mozart
    sentence: FINALMENTE, LA VOCE! A VOLTE MELODICA, A VOLTE SELVAGGIA E URLATA,
      TRASMETTE EMOZIONI INTENSE E SENZA FRENI.
  - characterName: Cadence
    sentence: "POSSO SENTIRE L\u2019ENERGIA! LA VOCE NEL METAL \xC8 CRUDA, EMOZIONALE
      E INARRESTABILE. \xC8 INTENSA E FORTE, SPESSO RUGGISCE CON UNA TALE FORZA DA
      SCUOTERE L\u2019ARIA INTORNO A TE.\r\n"
  - characterName: Mozart
    sentence: "ORA TI TROVI DI FRONTE A UN AVVERSARIO FORMIDABILE: INFERNO RIFF!
      ATTACCA VELOCEMENTE PERCH\xC9, NEL METAL, LA VELOCIT\xC0 \xC8 TUTTO! LE DITA
      DEI CHITARRISTI CORRONO SULLE CORDE COME FULMINI, E IL RITMO INCESSANTE NON
      SI FERMA MAI!"
  - characterName: Cadence
    sentence: "VELOCIT\xC0? NESSUN PROBLEMA! DEVO SOLO TROVARE IL RITMO GIUSTO E
      RESTARE IN SINCRONIA!\r\n"
  - characterName: Mozart
    sentence: "MA ATTENTO ALLA SUA MOSSA SPECIALE: L'ACCORDO MINORE! QUESTO SUONO
      INGANNATOREMENTE SEMPLICE PU\xD2 COLPIRTI E STORDIRTI PER QUALCHE SECONDO!
      SE VUOI RESISTERE, DEVI SCHIVARE CON TEMPI PRECISI!"
  - characterName: Cadence
    sentence: "UN ACCORDO CHE STORDISCE? SIGNIFICA CHE NON POSSO SOTTOVALUTARLO.
      DOVR\xD2 TENERE LE ORECCHIE ALL\u2019ERTA E LE DITA PRONTE!"
  - characterName: Mozart
    sentence: "CHE LA MUSICA SIA CON TE! MOSTRA A INFERNO RIFF CHE MELODIA E ARMONIA
      POSSONO RESISTERE ANCHE ALLA FORZA PI\xD9 RIBELLE!"
  - characterName: Inferno
    sentence: CHI OSARE SFIDARE IL RE DEL METAL? PENSI DAVVERO DI POTER RESISTERE
      ALLA MIA TEMPESTA DI NOTE?
  - characterName: Cadence
    sentence: "NON HO PAURA DEL TUO RUGGITO! LA MIA MUSICA PU\xD2 AFFRONTARE QUALSIASI
      RITMO, ANCHE IL PI\xD9 FEROCE!"
  - characterName: Inferno
    sentence: "AH! VEDREMO SE LA TUA ARMONIA POTR\xC0 SOPRAVVIVERE AL MIO SUONO DISTORTO!
      PREPARATI AL MIO ASSOLO LETALE!\r\n"
  - characterName: Cadence
    sentence: "SE IL TUO ASSOLO SUONA COME UN TUONO, ALLORA IO SAR\xD2 LA MELODIA
      CHE LO TRAVOLGER\xC0!"
  - characterName: Inferno
    sentence: "BELLE PAROLE, MA NEL METAL LA POESIA NON BASTA! QUI SI COMBATTE CON
      RITMO, VELOCIT\xC0 E POTENZA!"
  - characterName: Cadence
    sentence: "E CHI HA DETTO CHE LA MIA MUSICA SIA PRIVA DI POTENZA? OSSERVA ATTENTAMENTE,
      PERCH\xC9 IL MIO CONTRATTACCO SAR\xC0 PI\xD9 FORTE DI QUANTO TI ASPETTI!"
